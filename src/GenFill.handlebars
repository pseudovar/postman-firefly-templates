{{>tailwind}}
<div class="px-8">
    <div class="mx-auto max-w-2xl px-4 py-16 sm:px-6 sm:py-24 lg:max-w-7xl">

        {{>header title="Firefly API Results"}}

        <div class="grid grid-cols-1 gap-y-4 sm:grid-cols-2 sm:gap-x-6 sm:gap-y-10 mb-8">
        {{#each images}}
            <div class="relative flex flex-col overflow-hidden rounded-lg bg-white">
                <div class="aspect-h-4 aspect-w-3 bg-gray-200 sm:aspect-none">
                    <img src="{{image.presignedUrl}}">
                </div>
                <div class="flex flex-1 flex-col space-y-2 p-4">
                    <h3 class="text-sm font-medium text-gray-900 text-center">Seed: {{seed}}</h3>
                </div>  
            </div>
        {{/each}}
        </div>
    </div>
</div>