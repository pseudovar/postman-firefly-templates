{{#*inline "h1"}}
<h1>{{title}}</h1>
{{/inline}}
{{#*inline "header"}}
<div class="flex items-center justify-between">
  <div class="min-w-0 flex-1">
    <h2 class="text-2xl font-bold leading-7 text-gray-900 sm:truncate sm:text-3xl sm:tracking-tight">{{title}}</h2>
  </div>
  <div class="mt-4 flex md:ml-4 md:mt-0">
    <a 
            href='https://developer.adobe.com/firefly-services/docs/firefly-api/' class="ml-3 inline-flex items-center rounded-md bg-indigo-600 px-3 py-2 text-sm font-semibold text-white shadow-sm hover:bg-indigo-700 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">API Docs</button>
  </div>
</div>
{{/inline}}
{{#*inline "tailwind"}}
<script src='https://cdn.tailwindcss.com'></script>
{{/inline}}
{{>tailwind}}
<div class="mx-auto max-w-2xl px-4 py-16 sm:px-6 sm:py-24 lg:max-w-7xl lg:px-8">

{{>header title="Firefly API Results"}}

<div class="grid grid-cols-1 gap-y-4 sm:grid-cols-2 sm:gap-x-6 sm:gap-y-10 lg:grid-cols-3 lg:gap-x-8 mb-8">
{{#each outputs}}
    <div class="group relative flex flex-col overflow-hidden rounded-lg bg-white">
        <div class="aspect-h-4 aspect-w-3 bg-gray-200 sm:aspect-none group-hover:opacity-75">
            <img src="{{image.presignedUrl}}">
        </div>
        <div class="flex flex-1 flex-col space-y-2 p-4">
            <h3 class="text-sm font-medium text-gray-900 text-center">Seed: {{seed}}</h3>
        </div>  
    </div>
{{/each}}
</div>
</div>